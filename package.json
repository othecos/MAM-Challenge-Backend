{
  "name": "man-challenge-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test npm run set-env && mocha --require ts-node/register --bail --exit --file 'src/tests/setup.spec.ts' 'src/**/**spec.ts' --reporter mochawesome --reporter-options reportDir=test-results,reportFilename=test-results",
    "set-env": "rm -rf .env && ts-node environments",
    "nodemon": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify 'src/' --exec 'npm run start:dev'",
    "prelocal": "cross-env NODE_ENV=local npm run set-env",
    "local": "npm run nodemon",
    "preprod": "cross-env NODE_ENV=production npm run set-env",
    "prod": "npm run nodemon",
    "start:dev": "ts-node src",
    "populate": "ts-node populate",
    "linter": "eslint src/**/*.ts --fix",
    "build": "rm -rf ./dist && tsc",
    "postbuild": "copyfiles -s 'src/**/*' -e 'src/**/*.ts' -u 1 dist",
    "start": "npm run server",
    "server": "node dist/index",
    "clean-install": "rm -rf node_modules && npm install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run linter"
    }
  },
  "author": "Otavio Henrique Pires Costa",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.14",
    "@types/chai": "^4.2.16",
    "@types/faker": "^5.5.1",
    "@types/jest": "^26.0.22",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "eslint": "^7.18.0",
    "eslint-plugin-mocha": "^8.1.0",
    "faker": "^5.5.3",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "ts-node-dev": "^1.1.6"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.1.16",
    "@hapi/joi": "^17.1.1",
    "@types/cors": "^2.8.9",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.11",
    "@types/hapi__joi": "^17.1.6",
    "@types/mongoose": "^5.10.3",
    "@types/morgan": "^1.9.2",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-joi-validation": "^4.0.3",
    "husky": "^4.3.8",
    "joi": "^17.3.0",
    "mochawesome": "^6.2.2",
    "mongoose": "^5.11.13",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "swagger-jsdoc": "^6.0.1",
    "swagger-ui-express": "^4.1.6",
    "tsconfig-paths": "^3.9.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "winston": "^3.3.3"
  }
}
